{
    "version": 2,
    "builds": [
      {
        "src": "Server.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Server.js"
      }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; script-src 'self' https://vercel.live; img-src 'self' https://api-jadwal.vercel.app;"
                }
            ]
        }
    ]
  }
  
  